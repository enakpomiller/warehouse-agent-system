  <div class="container" style="margin-top:20px;margin-bottom:50px;">
 <h3> Agent to Ware House  </h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Product Image</th>
      <th scope="col">Product Name</th>
      <th scope="col">Product  Price</th>
      <th scope="col">Product Brand</th>
      <th scope="col">Customer Price</th>
    </tr>
  </thead>
  <tbody>
    {{#each getclientmsg}}
    <tr>
      <td width="20%"><img src="http://localhost:4500/{{_previousDataValues.path}}" style="width:10%;"><br></td>
      <form class="row g-3 needs-validation" method="post" action="/agent/agent_nego"> 
      <input type="hidden" name="prod_id" value="{{_previousDataValues.id}}">
      <td><input type="text" style="border:0px solid red;" class="pt-2 pb-2 mt-2 mb-2 " readonly name="prod_name" value="{{_previousDataValues.prod_name}}"></td>
      <td><input type="text" style="border:0px solid red;" class="pt-2 pb-2 mt-2 mb-2 " readonly name="prod_price" value="{{_previousDataValues.prod_price}}"></td>
      <td><input type="text" style="border:0px solid red;" class="pt-2 pb-2 mt-2 mb-2 " readonly name="prod_brand" value="{{_previousDataValues.prod_brand}}"></td>
      <td> <input type="text"  type="text" style="border:0px solid red;" class="pt-2 pb-2 mt-2 mb-2 text-center"  readonly required name="cust_price" value="{{_previousDataValues.cust_price}}" ></td>
      <td><input type="hidden" style="border:0px solid red;" class="pt-2 pb-2 mt-2 mb-2 " readonly name="token" value="{{_previousDataValues.token}}"></td>
      <input type="hidden" name="path" value="{{_previousDataValues.path}}">
      <td>
          <a href="/agent/deleterec/{{_previousDataValues.id}}"  class="btn btn-primary pt-2 pb-2" onclick="return confirm(' do you wish to delete')"> Edit </a>
        <a href="/agent/deleterec/{{_previousDataValues.id}}"  class="btn btn-danger pt-2 pb-2" onclick="return confirm(' do you wish to delete')"> Delete </a>
      
      </td>
      </form>
    </tr>
     {{/each}}

  </tbody>
</table>
