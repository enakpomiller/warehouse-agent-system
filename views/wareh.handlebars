            <h4> Upload Product  </h4>
        <form action="/agent/wareh" enctype="multipart/form-data" method="POST">
                 <div class="form-group">
                   <input type="text" name="prod_name" class="form-control mt-3 mb-3 pt-2 pb-2" placeholder=" Product Name" style="width:100%">
                </div>
               <div class="form-group">
                   <input type="text" name="prod_price" class="form-control mt-3 mb-3 pt-2 pb-2" placeholder=" pRODUCT pRICE" style="width:100%">
                </div>
               <div class="form-group">
                   <input type="text" name="prod_brand" class="form-control mt-3 mb-3 pt-2 pb-2" placeholder=" Product Brand" style="width:100%">
                </div>
               <div class="form-group">
                   <input type="text"  name="prod_weight" class="form-control mt-3 mb-3 pt-2 pb-2" placeholder=" Product Weight" style="width:100%">
                </div>
               <div class="form-group">
                   <input type="file"  name="filename"  class="form-control mt-3 mb-3 pt-2 pb-2" placeholder=" Enter Firstname" style="width:100%">
                </div>
                <div class="form-group">
                    <select name="category" class="form-control mt-3 mb-3 pt-2 pb-2">
                        <option> select ware house</option>
                        <option cvalue="w1"> Ware House 1</option>
                        <option value="w2"> Ware house 2</option>
                        <option value="w3"> ware house 3</option>
                    </select>
                </div>
               <div class="form-group">
                   <input type="date" name="date" class="form-control mt-3 mb-3 pt-2 pb-2" placeholder=" Enter Firstname" style="width:100%">
                </div>
            <button class="btn-cust-reg" style="width:100%">  Upload Product </button>
            <a href="/agent/admin_nego"> Dashboard </a>
        </div>
</form>
