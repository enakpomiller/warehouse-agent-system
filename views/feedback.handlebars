<div class="container" style="margin-top:50px; margin-bottom:50px;" >
        <h3>  Feedback From Agent  </h3>
<table class="table table-hover">
  <thead class="thead-dark">
    <tr>
      <th scope="col"> Product Image </th>
      <th scope="col">PROD NAME </th>
      <th scope="col">PROD PRICE </th>
      <th scope="col">PROD BRAND </th>
      <th scope="col"> YOUR PRICE </th>
     <th scope="col">  WARE HOUSE REMARKS</th>
      <th scope="col">  ACTION</th>
    </tr>
  </thead>
  <tbody>
     {{#if GetCust}}
  {{#each GetCust}}
    <tr> 

     <td width="10%"><img src="http://localhost:4500/{{_previousDataValues.path}}" style="width:25%;"><br></td>
      <td>{{_previousDataValues.prod_name}}</td>
      <td>{{_previousDataValues.prod_price}}</td>
      <td>{{_previousDataValues.prod_brand}}</td>
      <td>{{_previousDataValues.cust_price}}</td>
      <td>{{_previousDataValues.decision}}</td>
       <td> <button class="btn btn-primary" onclick="print()"> Print </button></td>

    </tr>
    {{/each}}
  {{else}}
    no product 
  {{/if}}
  </tbody>
</table>
